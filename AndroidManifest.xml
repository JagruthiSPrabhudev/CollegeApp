<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sit.collegeapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.INTERNET" >
    </uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.sit.collegeapp.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sit.collegeapp.MainPage"
            android:label="@string/title_activity_main_page" >
        </activity>
        <activity
            android:name="com.sit.collegeapp.AboutUs"
            android:label="@string/title_activity_about_us"
            android:parentActivityName="com.sit.collegeapp.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sit.collegeapp.MainActivity" />
        </activity>
        <activity
            android:name="com.sit.collegeapp.Attendance"
            android:label="@string/title_activity_attendance" >
        </activity>
        <activity
            android:name="com.sit.collegeapp.ResultPage"
            android:label="@string/title_activity_resultpage" >
        </activity>
        <activity
            android:name="com.sit.collegeapp.NotificationPage"
            android:label="@string/title_activity_notificationpage" >
        </activity>
        <activity
            android:name="com.sit.collegeapp.StaffPage"
            android:label="@string/title_activity_staff_page"
            android:parentActivityName="com.sit.collegeapp.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.sit.collegeapp.MainActivity" />
        </activity>
          <activity
            android:name="com.sit.collegeapp.AboutCollege"
            android:label="@string/title_activity_aboutcollegepage" >
        </activity>
          <activity
            android:name="com.sit.collegeapp.BranchesPage"
            android:label="@string/title_activity_branchespage" >
        </activity>
                <activity
            android:name="com.sit.collegeapp.FaciPage"
            android:label="@string/title_activity_facipae" >
        </activity>
    </application>

</manifest>